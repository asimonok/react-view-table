{
  "name": "react-view-table",
  "version": "0.1.3",
  "description": "Simple react table",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "build:docs": "react-scripts build && mv build docs",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asimonok/react-view-table.git"
  },
  "keywords": [
    "react",
    "table"
  ],
  "author": "Alex Simonok <simonok.sasha@mail.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asimonok/react-view-table/issues"
  },
  "homepage": "https://github.com/asimonok/react-view-table#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-scripts": "1.1.5"
  }
}
